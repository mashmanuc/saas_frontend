{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "i18n:check": "node scripts/i18n-check.mjs",
    "check:no-raw-axios": "node scripts/check-no-raw-axios.mjs",
    "prepare": "husky || true",
    "test": "vitest run",
    "test:unit": "vitest",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
